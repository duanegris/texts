#----------------------------------------------
all : serveur client
#----------------------------------------------
serveur : serveur.c
	gcc -Wall -o serveur  serveur.c #-lnsl
#----------------------------------------------
client : client.c 
	gcc -Wall -o client client.c -lrpcsvc -lnsl
#----------------------------------------------
clean :
	/bin/rm -f *.o serveur client *~ pok* *.ps
