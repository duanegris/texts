rm -f *~ 
(for i in *.java
do 
	echo "===============================" 
      echo $i
      echo "===============================" 
      cat $i
done) > listing~
mv listing~ listing.java
a2ps -1 --line-numbers=2 -b=" " --left-title=" " --left-footer=" " --footer=" " --right-title=" " --center-title="Exemples RMI" -L 88 --right-footer=" " listing.java -o listing.ps
mv listing.java listing.java~ 
